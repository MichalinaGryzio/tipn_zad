    "def silnia():\n",
    "    print ('podaj liczbę')\n",
    "    liczba = float(input())\n",
    "    liczbai = int(liczba)\n",
    "    silnia = 1\n",
    "    while liczbai > 0:\n",
    "        silnia = silnia*liczbai\n",
    "        liczbai -= 1\n",
    "    print ('silnia %d = %d' % (liczba, silnia))\n"
